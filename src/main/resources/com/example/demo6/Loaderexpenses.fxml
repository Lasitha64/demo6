        <Pane layoutX="277.0" layoutY="101.0" prefHeight="398.0" prefWidth="294.0" styleClass="formbox">
            <children>
                <Button fx:id="btn_add1" alignment="CENTER" graphicTextGap="12.0" layoutX="29.0" layoutY="73.0" mnemonicParsing="false" onAction="#Add" prefHeight="45.0" prefWidth="236.0" styleClass="button1" text="View Food Expenses" textOverrun="WORD_ELLIPSIS" wrapText="true">
                    <font>
                        <Font name="System Bold" size="17.0" />
