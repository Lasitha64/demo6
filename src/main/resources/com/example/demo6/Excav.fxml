                <VBox layoutX="30.0" layoutY="14.0" prefHeight="296.0" prefWidth="157.0" spacing="30.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="25.0" prefWidth="162.0" styleClass="label1" text="ID" textFill="#000000eb">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <Label alignment="CENTER" prefHeight="25.0" prefWidth="162.0" styleClass="label1" text="Name " textFill="#000000eb">
