<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@stylesheet/Excav-main.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.LoaderRepair">

    <children>
        <Label alignment="CENTER" layoutX="66.0" layoutY="14.0" prefHeight="110.0" prefWidth="682.0" stylesheets="@stylesheet/Excav-main.css" text="Stationary Details" textAlignment="CENTER" textFill="#aaaaaade">
            <font>
                <Font name="Britannic Bold" size="48.0" />
            </font>
         <graphic>
            <ImageView blendMode="MULTIPLY" fitHeight="90.0" fitWidth="90.0">
               <image>
                  <Image url="@img/stationary.jfif" />
               </image>
            </ImageView>
         </graphic>
        </Label>
        <Button fx:id="btn_back" layoutX="25.0" layoutY="539.0" mnemonicParsing="false" onAction="#Back" prefHeight="46.0" prefWidth="116.0" styleClass="btnback" text="Back" wrapText="true">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
            <graphic>
                <ImageView blendMode="MULTIPLY" fitHeight="28.0" fitWidth="27.0">
                    <image>
                        <Image url="@img/arrow-left.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Pane layoutX="93.0" layoutY="109.0" prefHeight="423.0" prefWidth="630.0" styleClass="formbox">
            <children>
                <Button fx:id="btn_gen" alignment="CENTER" graphicTextGap="8.0" layoutX="197.0" layoutY="223.0" mnemonicParsing="false" onAction="#Generate" prefHeight="46.0" prefWidth="264.0" text="View Stationary Details" wrapText="true">
                    <font>
                        <Font name="System Bold" size="17.0" />
                    </font>
                    <graphic>
                        <ImageView blendMode="MULTIPLY" fitHeight="30.0" fitWidth="31.0">
                            <image>
                                <Image url="@img/report.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="btn_add1" alignment="CENTER" graphicTextGap="12.0" layoutX="197.0" layoutY="73.0" mnemonicParsing="false" onAction="#Add" prefHeight="46.0" prefWidth="264.0" text="Add Stationary Details" textOverrun="WORD_ELLIPSIS" wrapText="true">
                    <graphic>
                        <ImageView blendMode="MULTIPLY" fitHeight="35.0" fitWidth="38.0">
                            <image>
                                <Image url="@img/add.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font name="System Bold" size="17.0" />
                    </font>
                </Button>
            </children>
        </Pane>
    </children>


</AnchorPane>
