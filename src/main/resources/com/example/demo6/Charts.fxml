<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.Charts">
   <children>
      <Pane layoutX="224.0" layoutY="15.0" prefHeight="570.0" prefWidth="561.0" styleClass="formbox">
         <children>
            <BarChart fx:id="crusherChart" animated="false" barGap="5.0" categoryGap="5.0" layoutX="20.0" layoutY="77.0" prefHeight="416.0" prefWidth="522.0">
              <xAxis>
                <CategoryAxis fx:id="cruQuanY" animated="false" side="BOTTOM" tickLabelFill="#080808" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="cruNamX" side="LEFT" />
              </yAxis>
            </BarChart>
            <Label layoutX="163.0" layoutY="14.0" prefHeight="43.0" prefWidth="259.0" text="Spare Parts Available" textFill="#201f1f">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label layoutX="-18.0" layoutY="267.0" prefHeight="17.0" prefWidth="94.0" rotate="-90.0" text="Quantity" textFill="#030303" />
            <Label layoutX="267.0" layoutY="493.0" prefHeight="17.0" prefWidth="94.0" text="Name" textFill="#232323" />
         </children>
      </Pane>
      <Button fx:id="btn_back" layoutX="23.0" layoutY="551.0" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="94.0" styleClass="btnback" text="Back" />
      <Button fx:id="btn_generateReports" layoutX="14.0" layoutY="41.0" mnemonicParsing="false" onAction="#generateReports" prefHeight="34.0" prefWidth="195.0" styleClass="btnback" text="Generate Reports" />
   </children>
</AnchorPane>
