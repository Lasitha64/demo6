<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@stylesheet/employeeUpdate.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.EmployeeUpdate">
