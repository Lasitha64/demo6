<AnchorPane prefHeight="600.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.FinanceProfitOrLossAccountView">
    <children>
        <Label alignment="CENTER" layoutX="154.0" layoutY="14.0" prefHeight="82.0" prefWidth="491.0" text=" Profit or Loss Accont" textAlignment="RIGHT" textFill="#dbdbe6" wrapText="true">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Pane layoutX="20.0" layoutY="90.0" prefHeight="402.0" prefWidth="776.0" styleClass="formbox" stylesheets="@stylesheet/Finance.css">
         <children>
            <TableView fx:id="POLacc" layoutX="6.0" layoutY="9.0" prefHeight="384.0" prefWidth="768.0">
              <columns>
                  <TableColumn fx:id="POLid" minWidth="3.0" prefWidth="67.0" text="ID" />
                <TableColumn fx:id="Date" prefWidth="82.0" text="Date" />
                <TableColumn fx:id="Sales" prefWidth="113.0" text="Sales" />
                  <TableColumn fx:id="COS" minWidth="3.0" prefWidth="96.0" text="Cost of sales" />
                  <TableColumn fx:id="OI" minWidth="3.0" prefWidth="69.0" text="Oth.In" />
                  <TableColumn fx:id="OX" minWidth="3.0" prefWidth="68.0" text="Oth.exp" />
                  <TableColumn fx:id="GrossProfit" prefWidth="141.0" text="Gross profit" />
                  <TableColumn fx:id="NetProfit" prefWidth="90.0" text="Net profit" />
              </columns>
            </TableView>
         </children>
        </Pane>
        <Button fx:id="btn_back" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="96.0" layoutY="525.0" mnemonicParsing="false" onAction="#back_to_a" prefHeight="43.0" prefWidth="195.0" styleClass="btnback" text="Finance Home">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
            <graphic>
                <ImageView blendMode="MULTIPLY" fitHeight="23.0" fitWidth="24.0">
                    <image>
                        <Image url="@img/arrow-left.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="graphs" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="409.0" layoutY="525.0" mnemonicParsing="false" onAction="#move_to_j" prefHeight="43.0" prefWidth="298.0" styleClass="btnback" text="Graphical analysis of the profits">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
    </children>
    <stylesheets>
        <URL value="@stylesheet/Finance.css" />
    </stylesheets>
</AnchorPane>
