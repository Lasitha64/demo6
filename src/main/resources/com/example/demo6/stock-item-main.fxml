<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600" prefWidth="800" stylesheets="@stylesheet/stock-item-main.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.StockItemMain">
    <children>
        <Label layoutX="265.0" layoutY="22.0" prefHeight="70.0" prefWidth="282.0" text="STOCK ITEM" textFill="#000000e7">
            <font>
                <Font name="System Bold" size="48.0" />
            </font>
        </Label>
        <VBox layoutX="148.0" layoutY="130.0" prefHeight="359.0" prefWidth="512.0" spacing="80.0" styleClass="formbox">
            <children>
                <Button fx:id="buutn_view" mnemonicParsing="false" onAction="#View" prefHeight="52.0" prefWidth="237.0" text="VIEW  STOCK ITEM">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="140.0" top="80.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="buttn_add" mnemonicParsing="false" onAction="#Add" prefHeight="52.0" prefWidth="299.0" text="ADD NEW STOCK ITEMS">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                    <VBox.margin>
                        <Insets left="110.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
        <Button fx:id="buttn_back" layoutX="88.0" layoutY="521.0" mnemonicParsing="false" onAction="#Back" styleClass="btnback" text="BACK">
            <graphic>
                <ImageView blendMode="MULTIPLY" fitHeight="37.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@img/arrow-left.png" />
                    </image>
                </ImageView>
            </graphic>
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
