<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.FinanceProfitGraphs">
    <children>
        <Label alignment="CENTER" layoutX="125.0" layoutY="14.0" prefHeight="82.0" prefWidth="551.0" text="Graphical analysis of the profits" textAlignment="RIGHT" textFill="#dbdbe6" wrapText="true">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Button fx:id="Home" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="622.0" layoutY="489.0" mnemonicParsing="false" onAction="#move_to_a" prefHeight="43.0" prefWidth="109.0" styleClass="btnback" text="Home">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
        <Button fx:id="btn_back" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="74.0" layoutY="489.0" mnemonicParsing="false" onAction="#back_to_i" prefHeight="43.0" prefWidth="102.0" styleClass="btnback" text="Back">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
            <graphic>
                <ImageView blendMode="MULTIPLY" fitHeight="23.0" fitWidth="24.0">
                    <image>
                        <Image url="@img/arrow-left.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
      <Button fx:id="Sales" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="262.0" layoutY="164.0" mnemonicParsing="false" onAction="#move_to_k" prefHeight="43.0" prefWidth="298.0" styleClass="btnback" text="Sales" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <padding>
            <Insets right="120.0" />
         </padding>
      </Button>
      <Button fx:id="GrossProfit" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="262.0" layoutY="249.0" mnemonicParsing="false" onAction="#move_to_l" prefHeight="43.0" prefWidth="298.0" styleClass="btnback" text="Gross profit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <padding>
            <Insets right="100.0" />
         </padding>
      </Button>
      <Button fx:id="NetProfit" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="262.0" layoutY="333.0" mnemonicParsing="false" onAction="#move_to_m" prefHeight="43.0" prefWidth="298.0" styleClass="btnback" text="Net profit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <padding>
            <Insets right="110.0" />
         </padding>
      </Button>
    </children>
    <stylesheets>
        <URL value="@stylesheet/Finance.css" />
    </stylesheets>
</AnchorPane>
