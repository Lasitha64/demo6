<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@stylesheet/employeeDetails.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.EmployeeDetails">
   <children>
      <Label alignment="CENTER" layoutX="107.0" layoutY="46.0" prefHeight="88.0" prefWidth="675.0" text="Employee Details" textFill="#aaaaaa">
         <font>
            <Font name="Britannic Bold" size="48.0" />
         </font>
      </Label>
      <Pane layoutX="64.0" layoutY="208.0" prefHeight="283.0" prefWidth="694.0" styleClass="formbox">
         <children>
            <Button fx:id="addnewemployeebutton" layoutX="55.0" layoutY="47.0" mnemonicParsing="false" onAction="#addnewemployee" prefHeight="46.0" prefWidth="239.0" text="Add New Employee">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Button>
            <Button fx:id="searchemployee" layoutX="397.0" layoutY="48.0" mnemonicParsing="false" onAction="#searchemployee" prefHeight="46.0" prefWidth="239.0" styleClass="button1" text="Search Employee">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Button>
            <Button fx:id="employeeattendance" layoutX="223.0" layoutY="148.0" mnemonicParsing="false" onAction="#employeeattendance" prefHeight="46.0" prefWidth="239.0" text="Employee Attendance">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Button fx:id="backbutton" alignment="CENTER_RIGHT" layoutX="68.0" layoutY="546.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="85.0" styleClass="back" text="BACK" textFill="#f5f4f4">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <graphic>
            <ImageView blendMode="MULTIPLY" fitHeight="25.0" fitWidth="23.0">
               <image>
                  <Image url="@img/arrow-left.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="mainmenu" layoutX="665.0" layoutY="547.0" mnemonicParsing="false" onAction="#mainmenu" styleClass="back" text="Main Menu">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ImageView blendMode="MULTIPLY" fitHeight="71.0" fitWidth="77.0" layoutX="181.0" layoutY="56.0">
         <image>
            <Image url="@img/employee.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
