<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.FinanceSalesJournalView">
    <children>
        <Label alignment="CENTER" layoutX="154.0" layoutY="14.0" prefHeight="82.0" prefWidth="491.0" text="Sales Journal" textAlignment="RIGHT" textFill="#dbdbe6" wrapText="true">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Pane layoutX="16.0" layoutY="90.0" prefHeight="402.0" prefWidth="757.0" styleClass="formbox" stylesheets="@stylesheet/Finance.css">
            <children>
                <TableView fx:id="SJ" layoutX="23.0" layoutY="9.0" prefHeight="384.0" prefWidth="694.0">
                    <columns>
                  <TableColumn fx:id="SJID" prefWidth="96.0" text="Date" />
                        <TableColumn fx:id="Date" prefWidth="105.0" text="Dis" />
                        <TableColumn fx:id="InvoiceNo" prefWidth="116.0" text="Invoice No" />
                        <TableColumn fx:id="Customer" prefWidth="153.0" text="Customer" />
                        <TableColumn fx:id="TotalValue" prefWidth="120.0" text="Total Value" />
                        <TableColumn fx:id="LedgerPage" prefWidth="98.0" text="Ledger page" />
                    </columns>
                </TableView>
            </children>
        </Pane>
        <Button fx:id="back" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="96.0" layoutY="525.0" mnemonicParsing="false" onAction="#back_to_n" prefHeight="43.0" prefWidth="115.0" styleClass="btnback" text="Back">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
            <graphic>
                <ImageView blendMode="MULTIPLY" fitHeight="23.0" fitWidth="24.0">
                    <image>
                        <Image url="@img/arrow-left.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="home" alignment="CENTER_RIGHT" graphicTextGap="10.0" layoutX="552.0" layoutY="525.0" mnemonicParsing="false" onAction="#back_to_a" prefHeight="43.0" prefWidth="155.0" styleClass="btnback" text="Finance Home">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
    </children>
    <stylesheets>
        <URL value="@stylesheet/Finance.css" />
    </stylesheets>
</AnchorPane>
